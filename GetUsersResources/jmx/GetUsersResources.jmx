          <stringProp name="filename"></stringProp>
          <stringProp name="script">import java.text.SimpleDateFormat; 
import java.util.Date; 
import org.apache.jmeter.util.JMeterUtils;

Date startTime = new Date();
SimpleDateFormat df = new SimpleDateFormat(&quot;MM/dd/yyyy HH:mm:ss.SSS&quot;); 
String formattedDate1 = df.format(startTime); 
vars.put(&quot;startTime&quot;,formattedDate1); 
print(&quot;the startTime: &quot; + vars.get(&quot;startTime&quot;));

Integer durationTime = new Integer(vars.get(&quot;duration&quot;));

print(&quot;duration: &quot; + durationTime);

Date endTime = new Date();
endTime.setTime(endTime.getTime()+durationTime);
String formattedDate = df.format(endTime); 
//vars.put(&quot;endTime&quot;, formattedDate);

String value=JMeterUtils.getPropDefault(&quot;gEndTime&quot;,&quot;&quot;);
JMeterUtils.setProperty(&quot;gEndTime&quot;, formattedDate);

print(&quot;setting the endTime: &quot; + props.get(&quot;gEndTime&quot;));
</stringProp>
        </BeanShellPreProcessor>
